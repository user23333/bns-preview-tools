<Window x:Class="Xylia.Preview.UI.Views.DefinitionManager"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" mc:Ignorable="d"
        xmlns:local="clr-namespace:Xylia.Preview.UI.Views"
        Title="DefinitionManager" Height="450" Width="800" Initialized="DefinitionManager_Initialized">
	<ListBox Name="CommitsList">
		<ListBox.ItemTemplate>
			<DataTemplate>
				<StackPanel>

					<TextBlock Text="{Binding Message}" />
					<TextBlock Text="{Binding Time}" />

					<Button Content="Download" Command="{Binding DownloadCommand}" />
				</StackPanel>
			</DataTemplate>
		</ListBox.ItemTemplate>
	</ListBox>
</Window>