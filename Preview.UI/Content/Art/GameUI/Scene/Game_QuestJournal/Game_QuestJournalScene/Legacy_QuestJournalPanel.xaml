<GameWindow x:Class="Xylia.Preview.UI.GameUI.Scene.Game_QuestJournal.Legacy_QuestJournalPanel"
	xmlns="https://github.com/xyliaup/bns-preview-tools"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:s="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:i="clr-namespace:Xylia.Preview.UI.Common.Interactivity"
	xmlns:data="clr-namespace:Xylia.Preview.Data.Models;assembly=Preview.Core"
	Title="QuestJournalPanel" Height="790" Width="800">
	<GameWindow.Resources>
		<s:ContextMenu x:Key="ItemMenu">
			<s:MenuItem Header="{s:DynamicResource PreviewBasic}" Command="{i:PreviewBasic}" CommandParameter="{Binding Path=.}"/>
		</s:ContextMenu>
		<s:ContextMenu x:Key="ExtractMenu">
			<s:MenuItem Click="Extract_QuestList_Click" Header="{s:DynamicResource Extract_Quest}" />
			<s:MenuItem Click="Extract_EpicQuestList_Click" Header="{s:DynamicResource Extract_QuestEpic}" />
		</s:ContextMenu>
		<s:Style x:Key="FilterOption" TargetType="s:ItemsControl">
			<s:Setter Property="HorizontalAlignment" Value="Left"/>
			<s:Setter Property="ItemsPanel">
				<s:Setter.Value>
					<s:ItemsPanelTemplate>
						<s:WrapPanel Orientation="Horizontal" MaxWidth="{Binding Width}" />
					</s:ItemsPanelTemplate>
				</s:Setter.Value>
			</s:Setter>
			<s:Setter Property="ItemTemplate">
				<s:Setter.Value>
					<s:DataTemplate>
						<s:CheckBox Content="{Binding}" Margin="0 0 5 0" Checked="SearcherOption_Checked" Unchecked="SearcherOption_Unchecked" />
					</s:DataTemplate>
				</s:Setter.Value>
			</s:Setter>
		</s:Style>
	</GameWindow.Resources>

	<s:TabControl x:Name="QuestJournal_Tab" >
		<s:TabItem x:Name="QuestJournal_Tab_RadioButton_1" Header="正在进行">
			<s:Grid x:Name="QuestJournal_ProgressQuest" Background="{s:DynamicResource RegionBrush}">
				<s:Grid.ColumnDefinitions>
					<s:ColumnDefinition Width="*" MinWidth="270"/>
					<s:ColumnDefinition Width="2*" />
				</s:Grid.ColumnDefinitions>

				<s:Border BorderThickness="1" BorderBrush="{s:DynamicResource BorderBrush}">
					<s:Grid>
						<s:Grid.RowDefinitions>
							<s:RowDefinition Height="Auto" />
							<s:RowDefinition Height="*" />
						</s:Grid.RowDefinitions>

						<!-- Searcher Setting -->
						<s:TextBox Name="SearcherRule" TextChanged="SearcherRule_TextChanged" PreviewMouseDown="SearcherRule_GotFocus" LostFocus="SearcherRule_LostFocus" />
						<s:Popup Name="SearcherOption" PlacementTarget="{Binding ElementName=SearcherRule}" MouseEnter="SearcherOption_MouseEnter" MouseLeave="SearcherOption_MouseLeave">
							<s:Border Background="{s:DynamicResource ThirdlyRegionBrush}" Width="{Binding ActualWidth,ElementName=SearcherRule}" Padding="5 7">
								<VerticalBox>
									<s:TextBlock Text="{s:DynamicResource QuestJournal_Category}" FontWeight="Bold" Margin="0 0 0 3"/>
									<s:ItemsControl Name="Filter_Category" Style="{s:StaticResource FilterOption}" />

									<s:TextBlock Text="{s:DynamicResource QuestJournal_ContentType}" FontWeight="Bold" Margin="0 5 0 3"/>
									<s:ItemsControl Name="Filter_ContentType" Style="{s:StaticResource FilterOption}" />

									<s:TextBlock Text="{s:DynamicResource QuestJournal_ResetType}" FontWeight="Bold" Margin="0 5 0 3"/>
									<s:ItemsControl Name="Filter_ResetType" Style="{s:StaticResource FilterOption}" />
								</VerticalBox>
							</s:Border>
						</s:Popup>

						<!-- Quest List -->
						<s:TreeView Name="QuestJournal_ProgressQuestList" BorderThickness="0" SelectedItemChanged="QuestJournal_ProgressQuestList_SelectedItemChanged" 
									s:Grid.Row="1" s:ScrollViewer.HorizontalScrollBarVisibility="Disabled" 
									s:VirtualizingStackPanel.IsVirtualizing="True" s:VirtualizingStackPanel.VirtualizationMode="Recycling">
							<s:TreeView.ItemTemplate>
								<s:DataTemplate DataType="{x:Type data:Quest}">
									<s:Grid>
										<s:Grid.ColumnDefinitions>
											<s:ColumnDefinition Width="Auto" />
											<s:ColumnDefinition Width="Auto" />
											<s:ColumnDefinition />
										</s:Grid.ColumnDefinitions>

										<BnsCustomLabelWidget s:Grid.Column="0" String="{Binding Path=Attributes[id]}" FontSize="12" Margin="-20 0 3 0" />
										<BnsCustomImageWidget s:Grid.Column="1" BaseImageProperty="{Binding Path=FrontIcon}" Width="20" Height="20" VerticalAlignment="Top" />
										<BnsCustomLabelWidget s:Grid.Column="2" String="{Binding Path=Name}" FontSize="14">
											<BnsCustomLabelWidget.Style>
												<s:Style TargetType="BnsCustomLabelWidget">
													<s:Style.Triggers>
														<s:DataTrigger Binding="{Binding Path=Attributes[retired]}" Value="True">
															<s:Setter Property="Foreground" Value="Red" />
														</s:DataTrigger>
													</s:Style.Triggers>
												</s:Style>
											</BnsCustomLabelWidget.Style>
										</BnsCustomLabelWidget>
									</s:Grid>
								</s:DataTemplate>
							</s:TreeView.ItemTemplate>
						</s:TreeView>
					</s:Grid>
				</s:Border>

				<!-- QuestInfo -->
				<s:ScrollViewer s:Grid.Column="2">
					<VerticalBox x:Name="QuestJournal_CurrentQuest_Info" Background="Transparent" ContextMenu="{s:StaticResource ItemMenu}" Margin="5 0 7 0">
						<BnsCustomLabelWidget String="{Binding Title, FallbackValue=Quest Information Title}" FontSize="17" Foreground= "Gray" />
						<HorizontalBox>
							<BnsCustomImageWidget BaseImageProperty="{Binding FrontIcon}" Width="20" Height="20" Margin="0 0 3 0"/>
							<BnsCustomLabelWidget String="{Binding Name}" FontSize="15" />
						</HorizontalBox>

						<!-- Content -->
						<BnsCustomLabelWidget x:Name="QuestJournal_Content" MetaData="textref=UI.QuestJournal.Info.Content" String="Content">
							<BnsCustomLabelWidget.Arguments>
								<ArgumentBinding>
									<Binding />
									<Binding />
								</ArgumentBinding>
							</BnsCustomLabelWidget.Arguments>
						</BnsCustomLabelWidget>

						<!-- Mission -->
						<BnsCustomLabelWidget x:Name="QuestJournal_Mission" MetaData="textref=UI.QuestJournal.Info.Mission" />
						<s:ItemsControl ItemsSource="{Binding MissionStep}">
							<s:ItemsControl.ItemTemplate>
								<s:DataTemplate>
									<VerticalBox>
										<BnsCustomLabelWidget MetaData="textref=UI.QuestJournal.Info.Step">
											<BnsCustomLabelWidget.Arguments>
												<ArgumentBinding>
													<Binding />
