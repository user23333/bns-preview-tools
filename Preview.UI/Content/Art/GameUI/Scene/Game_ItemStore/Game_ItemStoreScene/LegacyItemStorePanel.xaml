		<s:Grid s:DockPanel.Dock="Left" Width="300">
			<s:Grid.RowDefinitions>
				<s:RowDefinition Height="Auto" />
				<s:RowDefinition Height="*" />
			</s:Grid.RowDefinitions>

			<s:TextBox Name="SearcherRule" TextChanged="SearchStarted" />
			<s:TreeView s:Grid.Row="1" x:Name="TreeView" SelectedItemChanged="SelectedStoreChanged" ContextMenu="{s:StaticResource ExtractMenu}" s:ScrollViewer.HorizontalScrollBarVisibility="Disabled">
				<s:TreeView.ItemTemplate>
					<s:HierarchicalDataTemplate ItemsSource="{Binding Path=Items}">
						<s:DockPanel>
							<s:TextBlock Text="{Binding Name}" />
							<s:TextBlock Text="{Binding ItemCount,StringFormat=(0)}" s:DockPanel.Dock="Right" />
						</s:DockPanel>
						<s:HierarchicalDataTemplate.ItemTemplate>
							<s:DataTemplate>
								<HorizontalBox ToolTip="{Binding }">
									<BnsCustomLabelWidget String="{Binding Path=Name}" FontSize="14" Margin="0 0 5 0"/>
									<BnsCustomLabelWidget String="{Binding Path=Alias}" Foreground="{s:DynamicResource SecondaryTextBrush}" />
								</HorizontalBox>
							</s:DataTemplate>
						</s:HierarchicalDataTemplate.ItemTemplate>
					</s:HierarchicalDataTemplate>
				</s:TreeView.ItemTemplate>
			</s:TreeView>
		</s:Grid>

